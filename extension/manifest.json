{
	"manifest_version": 2,
	"name": "Web Log",
	"version": "0.1",
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"js": [
				"jquery-3.3.1.min.js",
				"content.js"
			]
		}
	],
	"background": {
		"scripts": [
			"background.js"
		],
		"persistent": false
	},
	"permissions": [
		"storage",
		"identity",
		"identity.email",
		"tabs"
	]
}